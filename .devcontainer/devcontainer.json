{
	"name": "Python 3",
	// Or use a Dockerfile or Docker Compose file. More info: https://containers.dev/guide/dockerfile
	"build": {
		"dockerfile": "../images/Dockerfile",
		"context": ".."
	},
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"features": {
		"ghcr.io/devcontainers/features/github-cli:1": {}
	},
	"postCreateCommand": "",
	"postAttachCommand": "gcloud config set project wb-ai-acltr-tbs-3-pr-a62583",
	"containerEnv": {
		"GOOGLE_APPLICATION_CREDENTIALS": "/workspaces/${localWorkspaceFolderBasename}/gcloud_cred.json",
		"PYTHONPATH": "/workspaces/${localWorkspaceFolderBasename}/stacks/b2b_ai/b2b_wf_prediction/src",
		"HTTP_PROXY": "http://198.161.14.25:8080",
		"HTTPS_PROXY": "http://198.161.14.25:8080",
		"http_proxy": "http://198.161.14.25:8080",
		"https_proxy": "http://198.161.14.25:8080"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"njpwerner.autodocstring",
				"ms-python.black-formatter",
				"waderyan.gitblame",
				"donjayamanne.githistory",
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"codezombiech.gitignore",
				"eamodio.gitlens",
				"ms-python.isort",
				"ms-python.vscode-pylance",
				"ms-python.python",
				"ms-python.debugpy",
				"ms-toolsai.jupyter",
				"donjayamanne.python-environment-manager",
				"ms-vscode.test-adapter-converter",
				"hbenl.vscode-test-explorer",
				"KevinRose.vsc-python-indent",
				"magicstack.MagicPython",
				"hashicorp.terraform",
				"yzhang.markdown-all-in-one",
				"ms-toolsai.vscode-jupyter-cell-tags"
			],
			"settings": {
				"files.autoSave": "onFocusChange",
				"editor.formatOnSave": true,
				"black-formatter.args": [
					"--line-length=79"
				],
				"terminal.integrated.defaultProfile.linux": "bash",
			}
		}
	}
}